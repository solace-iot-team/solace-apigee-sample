<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AM-SetJWKSUrl">
  <DisplayName>AM-SetJWKSURL</DisplayName>
  <Properties/>
  <AssignVariable>
    <Name>jwks.url</Name>
    <Template>{client.scheme}://{request.header.host}/oauth/jwks</Template>
  </AssignVariable>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
